[
  {
  "PropertyGroupId": "kinesis.analytics.flink.run.options",
  "PropertyMap": {
    "python": "DatastreamKafkaConnector/datastream-kafka-connector-example.py",
    "jarfile": "DatastreamKafkaConnector/lib/<Uber jar of flink-connector-kafka-1.15.2.jar & kafka-clients-3.3.1.jar>"
   }
  },
  {
    "PropertyGroupId": "consumer.config.0",
    "PropertyMap": {
      "input.topic": "event-consumer-topic",
      "bootstrap.servers": "localhost:9092",
      "aws.region": "us-east-1",
      "group.id": "test_group"
    }
  },
  {
    "PropertyGroupId": "producer.config.0",
    "PropertyMap": {
      "output.topic": "event-aggregator-topic",
      "bootstrap.servers": "localhost:9092",
      "aws.region": "us-east-1"
    }
  }
]